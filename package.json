{
  "name": "intygsadmin-root",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.6.2",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=10.0.0"
  },
  "scripts": {
    "start": "pnpm --filter intygsadmin start",
    "dev": "pnpm start",
    "build": "pnpm --filter intygsadmin build",
    "test": "pnpm --filter intygsadmin test",
    "client": "pnpm --filter intygsadmin",
    "install:client": "cd web/client && pnpm install",
    "clean": "pnpm --filter intygsadmin exec rm -rf node_modules && rm -rf node_modules"
  },
  "pnpm": {
    "overrides": {
      "abab": "2.0.6",
      "glob": "^10.0.0",
      "inflight": "npm:@nodelib/fs.walk@^2.0.0",
      "react": "^19.2.0",
      "react-dom": "^19.2.0"
    },
    "publicHoistPattern": [
      "*eslint*",
      "*prettier*"
    ]
  }
}
